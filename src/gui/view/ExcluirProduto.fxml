            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.controller.ExcluirProdutoController">
    <children>
        <Label layoutX="26.0" layoutY="23.0" text="Excluir Produto">
            <font>
                <Font size="27.0" />
            </font>
        </Label>
        <TextField fx:id="txtIdProduto" layoutX="29.0" layoutY="115.0" prefHeight="26.0" prefWidth="167.0" promptText="ID do Produto" />
        <Button fx:id="btnExcluirProduto" layoutX="26.0" layoutY="187.0" mnemonicParsing="false" text="Excluir" />
        <Button fx:id="btnCancelar" layoutX="131.0" layoutY="187.0" mnemonicParsing="false" text="Cancelar" />
    </children>
</AnchorPane>
