<AnchorPane prefHeight="550.0" prefWidth="500.0" style="-fx-background-color: #DAD5C0;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.CadastroProdutosController">
